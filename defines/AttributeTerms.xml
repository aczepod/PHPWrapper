<?xml version="1.0" encoding="UTF-8"?>
<terms>
    <!-- IDENTIFICATION ATTRIBUTES -->
    
    <!-- kTERM_NID -->
    <term lid="NID" ns="">
        <element variable="kTAG_LABEL" key="en">Native identifier</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[The <i>native identifier</i> uniquely
        	identifies an object <i>within its container</i>, this attribute is <i>specific
        	to the storage engine</i> and will be in general a native data type.
        	]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_NID</item>
        </element>
    </term>
    
    <!-- kTERM_LID -->
    <term lid="LID" ns="">
        <element variable="kTAG_LABEL" key="en">Local identifier</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[The <i>local identifier</i> uniquely
        	identifies an object <i>within a specific domain or namespace</i>. The value is
        	by definition a <i>string</i>.
        	]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_LID</item>
        </element>
    </term>
    
    <!-- kTERM_GID -->
    <term lid="GID" ns="">
        <element variable="kTAG_LABEL" key="en">Global identifier</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[The <i>global identifier</i> uniquely
        	identifies the object <i>across domains and namespaces</i>. The value is by
        	definition a <i>string</i>.
        	]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_GID</item>
        </element>
    </term>
    
    <!-- kTERM_UID -->
    <term lid="UID" ns="">
        <element variable="kTAG_LABEL" key="en">Unique identifier</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[The <i>unique identifier</i> is a coded
        	value that <i>uniquely identifies</i> an object in a container. This value will
        	generally be a hashed string.
        	]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_UID</item>
        </element>
    </term>
    
    <!-- kTERM_SYNONYMS -->
    <term lid="SYNONYMS" ns="">
        <element variable="kTAG_LABEL" key="en">Synonyms</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[The <i>synonyms</i> or <i>alternate
        	identifiers</i> are a list of strings by which an object is referenced. This
        	attribute is used to record all the identifiers which are not present in the
        	ontology; these strings <i>are not related to languages</i>.
        	]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_SYNONYMS</item>
        </element>
    </term>
    
    <!-- CLASSIFICATION ATTRIBUTES -->
    
    <!-- kTERM_CATEGORY -->
    <term lid="CATEGORY" ns="">
        <element variable="kTAG_LABEL" key="en">Category</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[The <i>category</i> is an <i>enumerated
        	set</i> of values which represent the <i>various categories</i> to which an
        	object belongs. The category is unrelated to the <i>class</i> of the object.
        	]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_CATEGORY</item>
        </element>
    </term>
    
    <!-- kTERM_KIND -->
    <term lid="KIND" ns="">
        <element variable="kTAG_LABEL" key="en">Kind</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[The <i>kind</i> is an <i>enumerated set</i>
        	of values which represent the <i>various types</i> of the referencing object.
        	The kind is specific to the <i>class</i> of the object.
        	]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_KIND</item>
        </element>
    </term>
    
    <!-- kTERM_TYPE -->
    <term lid="TYPE" ns="">
        <element variable="kTAG_LABEL" key="en">Type</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[The <i>type</i> is an <i>enumerated set</i>
        	of values which represent the <i>various types</i> of the referenced object.
        	The type applies to the referenced object and is used almost wxclusively to
        	indicate the <i>data type</i>.
        	]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_TYPE</item>
        </element>
    </term>
    
    <!-- kTERM_CLASS -->
    <term lid="CLASS" ns="">
        <element variable="kTAG_LABEL" key="en">Class</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[The <i>class</i> represents the object's
        	<i>class name</i>, it is generally used for instantiating the correct class once
			an object has been retrieved from its container.
        	]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_CLASS</item>
        </element>
    </term>
    
    <!-- kTERM_NAMESPACE -->
    <term lid="NAMESPACE" ns="">
        <element variable="kTAG_LABEL" key="en">Namespace</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[The <i>namespace</i> is a reference to a
        	term which represents a container for a set of other term identifiers.
        	Namespaces are used for grouping terms based on their functionality or to allow
        	homonym identifiers.
        	]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_NAMESPACE</item>
        </element>
    </term>
    
    <!-- DESCRIPTION ATTRIBUTES -->
    
    <!-- kTERM_LABEL -->
    <term lid="LABEL" ns="">
        <element variable="kTAG_LABEL" key="en">Label</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[The <i>label</i> is a string which
        	represents the <i>name</i> or <i>short description</i> of an object. It is a
        	structure which stores labels <i>expressed in different languages</i>.
        	]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_LABEL</item>
        </element>
    </term>
    
    <!-- kTERM_DEFINITION -->
    <term lid="DEFINITION" ns="">
        <element variable="kTAG_LABEL" key="en">Definition</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[The <i>definition</i> is the description
        	of an object <i>independent from its context</i>. It is a structure which stores
        	definitions <i>expressed in different languages</i>.
        	]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_DEFINITION</item>
        </element>
    </term>
    
    <!-- kTERM_DESCRIPTION -->
    <term lid="DESCRIPTION" ns="">
        <element variable="kTAG_LABEL" key="en">Description</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[The <i>description</i> is the definition
        	of an object <i>dependent on its context</i>. It is a structure which stores
        	descriptions <i>expressed in different languages</i>.
        	]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_DESCRIPTION</item>
        </element>
    </term>
    
    <!-- kTERM_NOTES -->
    <term lid="NOTES" ns="">
        <element variable="kTAG_LABEL" key="en">Notes</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[The <i>notes</i> or <i>comments</i> provide
        	additional information unrelated to the definition or description. It is a
        	structure which stores notes <i>expressed in different languages</i>.
        	]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_NOTES</item>
        </element>
    </term>
    
    <!-- kTERM_EXAMPLES -->
    <term lid="EXAMPLES" ns="">
        <element variable="kTAG_LABEL" key="en">Examples</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[The <i>examples</i> or <i>templates</i> are
        	a list of strings which represent use-cases. These strings <i>are not in any
        	specific language</i>.
        	]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_EXAMPLES</item>
        </element>
    </term>
    
    <!-- STATUS ATTRIBUTES -->
    
    <!-- kTERM_STATUS_LEVEL -->
    <term lid="STATUS-LEVEL" ns="">
        <element variable="kTAG_LABEL" key="en">Status level</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[The <i>severity</i> or <i>status level</i>,
        	is an element of an <i>enumerated set</i> which indicates the <i>importance</i>
        	or <i>severity</i> of an event. The attribute must be one of the following
        	values:<ul>
        	<li><tt>0</tt>: <i>Idle</i>. This code indicates no errors, this state can be
				equated to an idle state.
			<li><tt>10</tt>: <i>Notice</i>. This code indicates a notice. A notice is an
				informative message that does not imply an error, nor a situation that
				should be handled; it can be considered as statistical data.
			<li><tt>20</tt>: <i>Message</i>. This code indicates a message. A message is an
				informative message that is addressed to somebody, although it does not
				imply an error or warning, it was issued to a receiving party.
			<li><tt>30</tt>: <i>Warning</i>. This code indicates a warning. Warnings are
				informative data that indicate a potential problem, although they do not
				imply an error, they indicate a potential problem or an issue that should
				be addressed at least at a later stage.
			<li><tt>40</tt>: <i>Error</i>. This code indicates an error. Errors indicate
				that something prevented an operation from being performed, this does not
				necessarily mean that the whole process is halted, but that the results of
				an operation will not be as expected.
			<li><tt>50</tt>: <i>Fatal</i>. This code indicates a fatal error. Fatal errors
				are errors that result in stopping the whole process: in this case the
				error will prevent other operations from being performed and the whole
				process should be halted.
			<li><tt>60</tt>: <i>Bug</i>. This code indicates a bug. Bugs, as opposed to
				errors, result from internal causes independant from external factors. A
				bug indicates that an operation will never execute as stated, it does not
				necessarily mean that it is fatal, but rather that the behaviour of an
				operation does not correspond to its declaration.
			</ul>
            ]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_STATUS_LEVEL</item>
        </element>
    </term>
    
    <!-- kTERM_STATUS_CODE -->
    <term lid="STATUS-CODE" ns="">
        <element variable="kTAG_LABEL" key="en">Status code</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[The <i>status code</i> is an element of an
        	<i>enumerated set</i> which indicates the specific <i>state</i> of the event.
            ]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_STATUS_CODE</item>
        </element>
    </term>
    
    <!-- kTERM_STATUS_IDENTIFIER -->
    <term lid="STATUS-IDENTIFIER" ns="">
        <element variable="kTAG_LABEL" key="en">Status identifier</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[The <i>status identifier</i>
        	is returned by services that insert objects, it represents the newly created
        	object's native identifier.
            ]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_STATUS_IDENTIFIER</item>
        </element>
    </term>
    
    <!-- kTERM_STATUS_MESSAGE -->
    <term lid="STATUS-MESSAGE" ns="">
        <element variable="kTAG_LABEL" key="en">Status message</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[The <i>status message</i> is a string that
        	<i>describes the status of an event</i>. It is a structure which stores messages
        	<i>expressed in different languages</i>.
            ]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_STATUS_MESSAGE</item>
        </element>
    </term>
    
    <!-- AUTHORSHIP ATTRIBUTES -->
    
    <!-- kTERM_AUTHORS -->
    <term lid="AUTHORS" ns="">
        <element variable="kTAG_LABEL" key="en">Authors</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[It is a <i>list of author names</i>.
            ]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_AUTHORS</item>
        </element>
    </term>
    
    <!-- kTERM_ACKNOWLEDGMENTS -->
    <term lid="ACKNOWLEDGMENTS" ns="">
        <element variable="kTAG_LABEL" key="en">Acknowledgments</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[It is a <i>list of generic
        	acknowledgments</i>.
            ]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_ACKNOWLEDGMENTS</item>
        </element>
    </term>
    
    <!-- kTERM_BIBLIOGRAPHY -->
    <term lid="BIBLIOGRAPHY" ns="">
        <element variable="kTAG_LABEL" key="en">Bibliography</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[It is a <i>list of bibliographic
        	references</i>.
            ]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_BIBLIOGRAPHY</item>
        </element>
    </term>
    
    <!-- REFERENCE ATTRIBUTES -->
    
    <!-- kTERM_TERM -->
    <term lid="TERM" ns="">
        <element variable="kTAG_LABEL" key="en">Term</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[It is a <i>reference to a term</i>.
            ]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_TERM</item>
        </element>
    </term>
    
    <!-- kTERM_NODE -->
    <term lid="NODE" ns="">
        <element variable="kTAG_LABEL" key="en">Node</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[It is a <i>reference to a node</i>.
            ]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_NODE</item>
        </element>
    </term>
    
    <!-- kTERM_SUBJECT -->
    <term lid="SUBJECT" ns="">
        <element variable="kTAG_LABEL" key="en">Subject</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[It is the <i>subject of a relationship</i>,
        	or the <i>vertex from which a relationship originates</i>. This attribute is a
        	<i>node reference</i>.
            ]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_SUBJECT</item>
        </element>
    </term>
    
    <!-- kTERM_OBJECT -->
    <term lid="OBJECT" ns="">
        <element variable="kTAG_LABEL" key="en">Object</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[It is the <i>object of a relationship</i>,
        	or the <i>vertex to which the relationship points</i>. This attribute is a
        	<i>node reference</i>.
            ]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_OBJECT</item>
        </element>
    </term>
    
    <!-- kTERM_PREDICATE -->
    <term lid="PREDICATE" ns="">
        <element variable="kTAG_LABEL" key="en">Predicate</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[It is the <i>term</i> which <i>relates the
        	subject and object of a relationship</i>. This attribute is a <i>term
        	reference</i>.
            ]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_PREDICATE</item>
        </element>
    </term>
    
    <!-- kTERM_PATH -->
    <term lid="PATH" ns="">
        <element variable="kTAG_LABEL" key="en">Path</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[It is the <i>sequence</i> of <i>vertex</i>,
        	<i>predicate</i> and <i>vertex</i> elements which <i>connect</i> an
        	<i>origin vertex</i> to a <i>destination vertex</i>.
            ]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_PATH</item>
        </element>
    </term>
    
    <!-- kTERM_NAMESPACE_REFS -->
    <term lid="NAMESPACE-REFS" ns="">
        <element variable="kTAG_LABEL" key="en">Namespace references count</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[It is an <i>integer attribute</i> which
        	<i>counts</i> how many times a term was <i>referenced as a namespace</i>.
            ]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_NAMESPACE_REFS</item>
        </element>
    </term>

    <!-- REFERENCE COLLECTIONS -->
    
    <!-- kTERM_NODES -->
    <term lid="NODES" ns="">
        <element variable="kTAG_LABEL" key="en">Nodes</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[It is an <i>collection of node
        	references</i>.
            ]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_NODES</item>
        </element>
    </term>
    
    <!-- kTERM_EDGES -->
    <term lid="EDGES" ns="">
        <element variable="kTAG_LABEL" key="en">Edges</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[It is an <i>collection of edge
        	references</i>.
            ]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_EDGES</item>
        </element>
    </term>
    
    <!-- kTERM_FEATURES -->
    <term lid="FEATURES" ns="">
        <element variable="kTAG_LABEL" key="en">Features</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[It is an <i>collection of feature
        	references</i>, references to objects that have <i>referenced the current
        	object</i> as a <i>feature</i> or <i>trait</i>.
            ]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_FEATURES</item>
        </element>
    </term>
    
    <!-- kTERM_METHODS -->
    <term lid="METHODS" ns="">
        <element variable="kTAG_LABEL" key="en">Methods</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[It is an <i>collection of method
        	references</i>, references to objects that have <i>referenced the current
        	object</i> as a <i>method</i> or <i>modifier</i>.
            ]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_METHODS</item>
        </element>
    </term>
    
    <!-- kTERM_SCALES -->
    <term lid="SCALES" ns="">
        <element variable="kTAG_LABEL" key="en">Scales</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[It is an <i>collection of scale
        	references</i>, references to objects that have <i>referenced the current
        	object</i> as a <i>scale</i> or <i>unit</i>.
            ]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_SCALES</item>
        </element>
    </term>
    
    <!-- USER ATTRIBUTES -->
    
    <!-- kTERM_USER_NAME -->
    <term lid="USER-NAME" ns="">
        <element variable="kTAG_LABEL" key="en">User name</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[The full user name.]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_USER_NAME</item>
        </element>
    </term>
    
    <!-- kTERM_USER_CODE -->
    <term lid="USER-CODE" ns="">
        <element variable="kTAG_LABEL" key="en">User code</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[The code with which the user is known to the system.]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_USER_CODE</item>
        </element>
    </term>
    
    <!-- kTERM_USER_PASS -->
    <term lid="USER-PASS" ns="">
        <element variable="kTAG_LABEL" key="en">User password</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[The password with which the user is known to the system.]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_USER_PASS</item>
        </element>
    </term>
    
    <!-- kTERM_USER_MAIL -->
    <term lid="USER-MAIL" ns="">
        <element variable="kTAG_LABEL" key="en">User e-mail</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[The e-mail address of the user.]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_USER_MAIL</item>
        </element>
    </term>
    
    <!-- kTERM_USER_ROLE -->
    <term lid="USER-ROLE" ns="">
        <element variable="kTAG_LABEL" key="en">User roles</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[The roles assigned to the user.]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_USER_ROLE</item>
        </element>
    </term>
    
    <!-- kTERM_USER_PROFILE -->
    <term lid="USER-PROFILE" ns="">
        <element variable="kTAG_LABEL" key="en">User profile</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[The profile role name assigned to the user.]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_USER_PROFILE</item>
        </element>
    </term>
    
    <!-- kTERM_USER_MANAGER -->
    <term lid="USER-MANAGER" ns="">
        <element variable="kTAG_LABEL" key="en">User manager</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[Reference to the user that manages the current user.]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_USER_MANAGER</item>
        </element>
    </term>
    
    <!-- kTERM_USER_DOMAIN -->
    <term lid="USER-DOMAIN" ns="">
        <element variable="kTAG_LABEL" key="en">User domain</element>
        <element variable="kTAG_DEFINITION" key="en"><![CDATA[List of <i>domains</i> to which the user has access to.]]>
        </element>
        <element variable="kTAG_SYNONYMS">
            <item>kTERM_USER_DOMAIN</item>
        </element>
    </term>
</terms>