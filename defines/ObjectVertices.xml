    <!-- kTERM_GID -->
    <unit>
        <node class="COntologyAliasVertex">
            <element variable="kTAG_TERM">:GID</element>
            <element variable="kTAG_KIND">
                <item>:KIND-FEATURE</item>
                <item>:KIND-SCALE</item>
            </element>
            <element variable="kTAG_TYPE">
                <item>:TEXT</item>
                <item>:LOCKED</item>
                <item>:COMPUTED</item>
