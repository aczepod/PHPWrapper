<?xml version="1.0" encoding="UTF-8"?>
<units>
    <!-- TERM -->

    <!-- kTERM_NID -->
    <unit>
        <node class="COntologyAliasVertex">
            <element variable="kTAG_TERM">:NID</element>
            <element variable="kTAG_KIND">
                <item>:KIND-FEATURE</item>
                <item>:KIND-SCALE</item>
            </element>
            <element variable="kTAG_TYPE">
                <item>:BINARY</item>
            </element>
            <element variable="kTAG_DESCRIPTION" key="en"><![CDATA[The <i>native identifier</i> of a term is
				its <i>global identifier hashed</i> into a binary string.
				]]>
            </element>
        </node>
        <edges>
            <edge>
                <element variable="kTAG_PREDICATE">:ATTRIBUTE-OF</element>
                <element variable="kTAG_OBJECT">:_terms</element>
            </edge>
        </edges>
    </unit>

    <!-- kTERM_CLASS -->
    <unit>
        <node class="COntologyMasterVertex">
            <element variable="kTAG_TERM">:CLASS</element>
            <element variable="kTAG_KIND">
                <item>:KIND-FEATURE</item>
                <item>:KIND-SCALE</item>
            </element>
            <element variable="kTAG_TYPE">
                <item>:TEXT</item>
                <item>:LOCKED</item>
                <item>:COMPUTED</item>
            </element>
        </node>
        <edges>
            <edge>
                <element variable="kTAG_PREDICATE">:ATTRIBUTE-OF</element>
                <element variable="kTAG_OBJECT">:_terms</element>
            </edge>
        </edges>
    </unit>

    <!-- kTERM_LID -->
    <unit>
        <node class="COntologyAliasVertex">
            <element variable="kTAG_TERM">:LID</element>
            <element variable="kTAG_KIND">
                <item>:KIND-FEATURE</item>
                <item>:KIND-SCALE</item>
            </element>
            <element variable="kTAG_TYPE">
                <item>:TEXT</item>
                <item>:REQUIRED</item>
            </element>
            <element variable="kTAG_DESCRIPTION" key="en"><![CDATA[The <i>local identifier</i> of a term must
				be <i>unique within its namespace</i>, it is by definition a <i>string</i> and
				it is <i>required</i>.
				]]>
            </element>
        </node>
        <edges>
            <edge>
                <element variable="kTAG_PREDICATE">:ATTRIBUTE-OF</element>
                <element variable="kTAG_OBJECT">:_terms</element>
            </edge>
        </edges>
    </unit>

    <!-- kTERM_GID -->
    <unit>
        <node class="COntologyAliasVertex">
            <element variable="kTAG_TERM">:GID</element>
            <element variable="kTAG_KIND">
                <item>:KIND-FEATURE</item>
                <item>:KIND-SCALE</item>
            </element>
            <element variable="kTAG_TYPE">
                <item>:TEXT</item>
                <item>:LOCKED</item>
                <item>:COMPUTED</item>
                <item>:REQUIRED</item>
            </element>
            <element variable="kTAG_DESCRIPTION" key="en"><![CDATA[The <i>global identifier</i> of a term is
				computed by <i>concatenating</i> the eventual <i>namespace</i> term's <i>global
				identifier</i> to the current object's <i>local identifier</i>, separated by a
				<i>colon</i>. If the namespace is missing, the global identifier will be the
				local identifier.
				]]>
            </element>
        </node>
        <edges>
            <edge>
                <element variable="kTAG_PREDICATE">:ATTRIBUTE-OF</element>
                <element variable="kTAG_OBJECT">:_terms</element>
            </edge>
        </edges>
    </unit>

    <!-- kTERM_NAMESPACE -->
