    <!-- kTERM_LABEL -->
    <vertex kTAG_GID=":LABEL">
		<kTAG_CLASS>COntologyAliasVertex</kTAG_CLASS>
		<kTAG_KIND>
            <item>:KIND-FEATURE</item>
            <item>:KIND-SCALE</item>
        </kTAG_KIND>
		<kTAG_TYPE>
            <item>:LSTRING</item>
        </kTAG_TYPE>
        <kTAG_DESCRIPTION language="en"><![CDATA[The <i>label</i> of a term represents a
        	<i>common name</i> or <i>short description</i>. This attribute is a <i>list of
        	strings</i>, each representing the label in a <i>different language</i>. The
        	structure is an array in which the <i>key is the language code</i> and the
        	<i>value is the label</i>. This will be generally the <i>human readable name</i>
        	of the concept represented by the term.
            ]]>
        </kTAG_DESCRIPTION>
        <edges>
			<edge>
				<kTAG_PREDICATE>:ATTRIBUTE-OF</kTAG_PREDICATE>
				<kTAG_OBJECT>:_terms</kTAG_OBJECT>
			</edge>
        </edges>
