    <!-- kTERM_LID -->
    <vertex kTAG_GID=":LID">
		<kTAG_CLASS>COntologyAliasVertex</kTAG_CLASS>
		<kTAG_KIND>
            <item>:KIND-FEATURE</item>
            <item>:KIND-SCALE</item>
        </kTAG_KIND>
		<kTAG_TYPE>
            <item>:TEXT</item>
            <item>:REQUIRED</item>
        </kTAG_TYPE>
        <kTAG_DESCRIPTION language="en"><![CDATA[The <i>local identifier</i> of a term must
        	be <i>unique within its namespace</i>, it is by definition a <i>string</i> and
        	it is <i>required</i>.
            ]]>
        </kTAG_DESCRIPTION>
        <edges>
			<edge>
				<kTAG_PREDICATE>:ATTRIBUTE-OF</kTAG_PREDICATE>
				<kTAG_OBJECT>:_terms</kTAG_OBJECT>
			</edge>
        </edges>
    </vertex>
    
    <!-- kTERM_GID -->
    <vertex kTAG_GID=":GID">
		<kTAG_CLASS>COntologyAliasVertex</kTAG_CLASS>
		<kTAG_KIND>
            <item>:KIND-FEATURE</item>
            <item>:KIND-SCALE</item>
        </kTAG_KIND>
		<kTAG_TYPE>
            <item>:TEXT</item>
            <item>:LOCKED</item>
            <item>:COMPUTED</item>
            <item>:REQUIRED</item>
        </kTAG_TYPE>
        <kTAG_DESCRIPTION language="en"><![CDATA[The <i>global identifier</i> of a term is
        	computed by <i>concatenating</i> the eventual <i>namespace</i> term's <i>global
        	identifier</i> to the current object's <i>local identifier</i>, separated by a
        	<i>colon</i>. If the namespace is missing, the global identifier will be the
        	local identifier.
            ]]>
        </kTAG_DESCRIPTION>
        <edges>
			<edge>
				<kTAG_PREDICATE>:ATTRIBUTE-OF</kTAG_PREDICATE>
				<kTAG_OBJECT>:_terms</kTAG_OBJECT>
			</edge>
