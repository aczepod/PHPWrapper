        <node class="COntologyAliasVertex">
            <element variable="kTAG_TERM">:NAMESPACE</element>
            <element variable="kTAG_KIND">
                <item>:KIND-FEATURE</item>
                <item>:KIND-SCALE</item>
            </element>
            <element variable="kTAG_TYPE">
                <item>:BINARY</item>
            </element>
            <element variable="kTAG_DESCRIPTION" key="en"><![CDATA[The <i>namespace</i> of a term is a
				<i>reference</i> to another <i>term object</i> which acts as the namespace.
				]]>
            </element>
        </node>
        <edges>
            <edge>
                <element variable="kTAG_PREDICATE">:ATTRIBUTE-OF</element>
                <element variable="kTAG_OBJECT">:_terms</element>
            </edge>
        </edges>
    </unit>

    <!-- kTERM_SYNONYMS -->
    <unit>
        <node class="COntologyAliasVertex">
            <element variable="kTAG_TERM">:SYNONYMS</element>
