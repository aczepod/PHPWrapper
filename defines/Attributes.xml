    <!-- Identification attributes -->
    
	<!-- kTERM_NID -->
    <UNIT>
         <TERM NS="" LID="NID">
            <element variable="kTAG_LABEL">
                <item key="en">Native identifier</item>
            </element>
            <element variable="kTAG_DEFINITION">
                <item key="en"><![CDATA[The <i>native identifier</i> uniquely identifies the object <i>within its container</i>, this attribute is <i>specific to the storage engine</i> and will have in general a native data type.]]></item>
            </element>
            <element variable="kTAG_SYNONYMS">
                <item>kTERM_NID</item>
            </element>
        </TERM>
        <NODE class="COntologyMasterVertex">
        	<element variable="kTAG_CATEGORY">
        		<item>Identification</item>
        	</element>
            <element variable="kTAG_KIND">
                <item>:KIND-FEATURE</item>
                <item>:KIND-SCALE</item>
            </element>
            <element variable="kTAG_DESCRIPTION">
                <item key="en"><![CDATA[This attribute does not have a tag, since all objects share it.]]></item>
            </element>
        </NODE>
        <EDGE>
            <element variable="kTAG_PREDICATE">:SUBCLASS-OF</element>
            <element variable="kTAG_OBJECT">:ONTOLOGY:ATTRIBUTE</element>
        </EDGE>
    </UNIT>
    
