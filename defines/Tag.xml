<?xml version="1.0" encoding="UTF-8"?>
<ONTOLOGY
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="file:/Library/WebServer/Library/PHPWrapper/defines/Ontology.xsd">
    
    <!-- Tag object -->
    
    <UNIT>
        <TERM NS="" LID="_tags">
            <element variable="kTAG_LABEL">
                <item key="en">Tag</item>
            </element>
            <element variable="kTAG_DEFINITION">
                <item key="en"><![CDATA[An ontology tag represents the <i>path</i> between an <i>ontology feature vertex</i> and an <i>ontology scale vertex</i>. Tags record the terms referenced by the vertices and the predicates that relate them.]]></item>
            </element>
        </TERM>
        <NODE class="COntologyMasterVertex">
            <element variable="kTAG_CATEGORY">
                <item>Schema</item>
            </element>
        </NODE>
        <EDGE>
            <element variable="kTAG_PREDICATE">:SUBCLASS-OF</element>
            <element variable="kTAG_OBJECT">:ONTOLOGY:OBJECT</element>
        </EDGE>
    </UNIT>
    
	<!-- kTAG_NID -->
    <UNIT>
        <NODE class="COntologyAliasVertex" term=":NID">
            <element variable="kTAG_PID">:_tags.:NID</element>
            <element variable="kTAG_KIND">
                <item>:KIND-FEATURE</item>
                <item>:KIND-SCALE</item>
            </element>
            <element variable="kTAG_TYPE">
                <item>:INT</item>
                <item>:COMPUTED</item>
                <item>:REQUIRED</item>
            </element>
            <element variable="kTAG_DESCRIPTION">
                <item key="en"><![CDATA[The <i>native identifier</i> of a tag is an <i>integer</i> <i>generated by a sequence</i>. <i>All the attributes in this system are tagged with this attribute</i>, this is why it must be short.]]></item>
            </element>
        </NODE>
        <EDGE master="false">
            <element variable="kTAG_SUBJECT" node="pid">:_tags.:NID</element>
            <element variable="kTAG_PREDICATE">:ATTRIBUTE-OF</element>
            <element variable="kTAG_OBJECT">:_tags</element>
        </EDGE>
    </UNIT>
     
	<!-- kTAG_GID -->
    <UNIT>
        <NODE class="COntologyAliasVertex" term=":GID">
            <element variable="kTAG_PID">:_tags.:GID</element>
            <element variable="kTAG_KIND">
                <item>:KIND-FEATURE</item>
                <item>:KIND-SCALE</item>
            </element>
            <element variable="kTAG_TYPE">
                <item>:TEXT</item>
                <item>:LOCKED</item>
                <item>:COMPUTED</item>
                <item>:REQUIRED</item>
            </element>
            <element variable="kTAG_DESCRIPTION">
                <item key="en"><![CDATA[The <i>global identifier</i> of a tag is computed by <i>concatenating</i> the <i>term global identifiers</i> of all the <i>elements of the tag's path</i>, <i>separated by a slash</i>.]]></item>
            </element>
        </NODE>
        <EDGE master="false">
            <element variable="kTAG_SUBJECT" node="pid">:_tags.:GID</element>
            <element variable="kTAG_PREDICATE">:ATTRIBUTE-OF</element>
            <element variable="kTAG_OBJECT">:_tags</element>
        </EDGE>
    </UNIT>
   
