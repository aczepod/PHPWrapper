	<!-- kTAG_PATH -->
    <UNIT>
        <NODE class="COntologyAliasVertex" term=":PATH">
            <element variable="kTAG_PID">:_tags.:PATH</element>
            <element variable="kTAG_KIND">
                <item>:KIND-FEATURE</item>
                <item>:KIND-SCALE</item>
            </element>
            <element variable="kTAG_TYPE">
                <item>:BINARY</item>
                <item>:COMPUTED</item>
                <item>:LOCKED</item>
                <item>:ARRAY</item>
            </element>
            <element variable="kTAG_DESCRIPTION">
                <item key="en"><![CDATA[The <i>tag path</i> is a sequence of <i>vertex</i> elements separated by <i>predicate</i> elements, both vertices and predicates are term references. This attribute constitutes the unique identifier of a tag, the first vertex of the path is the <i>feature</i> of the tag, the last vertex of the path is the <i>scale</i> of the tag, which also provides the tag's data type.]]></item>
            </element>
        </NODE>
        <EDGE master="false">
            <element variable="kTAG_SUBJECT" node="pid">:_tags.:PATH</element>
            <element variable="kTAG_PREDICATE">:ATTRIBUTE-OF</element>
            <element variable="kTAG_OBJECT">:_tags</element>
        </EDGE>
    </UNIT>
    
