<?xml version="1.0" encoding="UTF-8"?>
<nodes>
    <!-- MAIN ONTOLOGY CATEGORY NODES -->
    
    <!-- kTERM_ONTOLOGY -->
	<node class="COntologyMasterVertex">
		<element variable="kTAG_TERM">:ONTOLOGY</element>
		<element variable="kTAG_CLASS">COntologyMasterVertex</element>
		<element variable="kTAG_KIND">
            <item>:KIND-ROOT</item>
        </element>
        <element variable="kTAG_DESCRIPTION"  key="en"><![CDATA[This represents the <i>default
        	ontology</i>, it defines all the elements that are used to build the ontology
        	itself.
        	]]>
        </element>
    </node>
    
    <!-- kTERM_NAMESPACES -->
	<node class="COntologyMasterVertex">
		<element variable="kTAG_TERM">:NAMESPACES</element>
		<element variable="kTAG_CLASS">COntologyMasterVertex</element>
        <element variable="kTAG_DESCRIPTION"  key="en"><![CDATA[This represents the <i>default
        	namespace</i>, it groups all identifiers which are used internally by the
        	ontology.
        	]]>
        </element>
    </node>
    
    <!-- kTERM_ATTRIBUTES -->
	<node class="COntologyMasterVertex">
		<element variable="kTAG_TERM">:ATTRIBUTES</element>
		<element variable="kTAG_CLASS">COntologyMasterVertex</element>
        <element variable="kTAG_DESCRIPTION"  key="en"><![CDATA[This is the collection of all <i>default
        	attributes</i>, these attributes are the properties of the internal objects of
        	this ontology engine.
        	]]>
        </element>
    </node>
    
    <!-- kTERM_PREDICATES -->
	<node class="COntologyMasterVertex">
		<element variable="kTAG_TERM">:PREDICATES</element>
		<element variable="kTAG_CLASS">COntologyMasterVertex</element>
        <element variable="kTAG_DESCRIPTION"  key="en"><![CDATA[This is the collection of all <i>default
        	predicates</i>, these represent the required predicates for building the
        	ontology.
        	]]>
        </element>
    </node>
    
    <!-- kTERM_ENUMERATIONS -->
	<node class="COntologyMasterVertex">
		<element variable="kTAG_TERM">:ENUMERATIONS</element>
		<element variable="kTAG_CLASS">COntologyMasterVertex</element>
        <element variable="kTAG_DESCRIPTION"  key="en"><![CDATA[This is the collection of all <i>default
        	enumerated sets</i>, these represent the required controlled vocabulary that
        	manages the ontology.
        	]]>
        </element>
    </node>
    
    <!-- ATTRIBUTE CATEGORY TERMS -->
    
    <!-- kTERM_IDENTIFICATION -->
	<node class="COntologyMasterVertex">
		<element variable="kTAG_TERM">:IDENTIFICATION</element>
		<element variable="kTAG_CLASS">COntologyMasterVertex</element>
        <element variable="kTAG_DESCRIPTION"  key="en"><![CDATA[This group of attributes allows <i>querying
        	ontology component collections</i> by <i>identifier</i> or <i>synonym</i>.
        	]]>
        </element>
    </node>
    
    <!-- kTERM_REFERENCE -->
	<node class="COntologyMasterVertex">
		<element variable="kTAG_TERM">:REFERENCE</element>
		<element variable="kTAG_CLASS">COntologyMasterVertex</element>
        <element variable="kTAG_DESCRIPTION"  key="en"><![CDATA[This group of attributes allows <i>cross
        	referencing</i> <i>terms</i>, <i>nodes</i>, <i>edges</i> and <i>tags</i>. These
        	references are not implemented as relationships, but as object properties.
        	]]>
        </element>
    </node>
    
    <!-- kTERM_CLASSIFICATION -->
	<node class="COntologyMasterVertex">
		<element variable="kTAG_TERM">:CLASSIFICATION</element>
		<element variable="kTAG_CLASS">COntologyMasterVertex</element>
        <element variable="kTAG_DESCRIPTION"  key="en"><![CDATA[This group of attributes provide a
        	<i>qualification</i> and <i>formal function</i> to the ontology objects, they
        	provide the rules of the behaviour of <i>terms</i>, <i>nodes</i>, <i>edges</i>
        	and <i>tags</i>.
        	]]>
        </element>
    </node>
    
    <!-- kTERM_REPRESENTATION -->
	<node class="COntologyMasterVertex">
		<element variable="kTAG_TERM">:REPRESENTATION</element>
		<element variable="kTAG_CLASS">COntologyMasterVertex</element>
        <element variable="kTAG_DESCRIPTION"  key="en"><![CDATA[<i>Representation attributes</i> are
        	<i>categorical qualifications</i> that provide indications on how to
        	<i>represent object properties</i> and on how to <i>validate their content</i>.
        	]]>
        </element>
    </node>
    
    <!-- kTERM_ILLUSTRATION -->
	<node class="COntologyMasterVertex">
		<element variable="kTAG_TERM">:ILLUSTRATION</element>
		<element variable="kTAG_CLASS">COntologyMasterVertex</element>
        <element variable="kTAG_DESCRIPTION"  key="en"><![CDATA[This group of attributes <i>describe</i>
        	and <i>name</i> the hosting objects, they provide a means to <i>illustrate</i>
        	and provide further details on objects and their context.
        	]]>
        </element>
    </node>
    
    <!-- kTERM_AUTHORSHIP -->
	<node class="COntologyMasterVertex">
		<element variable="kTAG_TERM">:AUTHORSHIP</element>
		<element variable="kTAG_CLASS">COntologyMasterVertex</element>
        <element variable="kTAG_DESCRIPTION"  key="en"><![CDATA[This group of attributes are used to
        	<i>reference</i> or <i>name</i> the <i>authors</i> or <i>origin</i> of the
        	elements of the ontology.
        	]]>
        </element>
    </node>
    
    <!-- kTERM_STATUS -->
	<node class="COntologyMasterVertex">
		<element variable="kTAG_TERM">:STATUS</element>
		<element variable="kTAG_CLASS">COntologyMasterVertex</element>
        <element variable="kTAG_DESCRIPTION"  key="en"><![CDATA[This group of attributes provide
        	<i>status</i> information about the outcome of an event or <i>state</i>
        	information on an object.
        	]]>
        </element>
    </node>
    
    <!-- ENUMERATION CATEGORY NODES -->
    
    <!-- kTERM_TYPES -->
	<node class="COntologyMasterVertex">
		<element variable="kTAG_TERM">:TYPES</element>
		<element variable="kTAG_CLASS">COntologyMasterVertex</element>
        <element variable="kTAG_DESCRIPTION"  key="en"><![CDATA[This group of enumerated sets covers the
        	<i>data types</i> that the ontology handles.
        	]]>
        </element>
    </node>
    
    <!-- kTERM_KINDS -->
	<node class="COntologyMasterVertex">
		<element variable="kTAG_TERM">:KINDS</element>
		<element variable="kTAG_CLASS">COntologyMasterVertex</element>
        <element variable="kTAG_DESCRIPTION"  key="en"><![CDATA[This controlled vocabulary collects all
        	the <i>categories</i> that define the <i>kind</i> of an <i>ontology
        	component</i>.
        	]]>
        </element>
    </node>
    
    <!-- kTERM_STATES -->
	<node class="COntologyMasterVertex">
		<element variable="kTAG_TERM">:STATES</element>
		<element variable="kTAG_CLASS">COntologyMasterVertex</element>
        <element variable="kTAG_DESCRIPTION"  key="en"><![CDATA[This enumerated set describes the
        	<i>status level</i> or <i>severity</i> of an event's outcome.
        	]]>
        </element>
        <element variable="kTAG_KIND">
            <item>:KIND-NAMESPACE</item>
        </element>
    </node>
    
    <!-- kTERM_OPERATORS -->
	<node class="COntologyMasterVertex">
		<element variable="kTAG_TERM">:OPERATORS</element>
		<element variable="kTAG_CLASS">COntologyMasterVertex</element>
        <element variable="kTAG_DESCRIPTION"  key="en"><![CDATA[This enumerated set represents the
        	<i>standard set of operators</i> which can be used to <i>query data
        	containers</i>.
        	]]>
        </element>
    </node>
    
    <!-- TYPE CATEGORY TERMS -->
    
    <!-- kTERM_PRIMITIVES -->
	<node class="COntologyMasterVertex">
		<element variable="kTAG_TERM">:PRIMITIVES</element>
		<element variable="kTAG_CLASS">COntologyMasterVertex</element>
        <element variable="kTAG_DESCRIPTION"  key="en"><![CDATA[These represent the <i>default primitive
        	data types</i>.
        	]]>
        </element>
    </node>
    
    <!-- kTERM_STRUCTURES -->
	<node class="COntologyMasterVertex">
		<element variable="kTAG_TERM">:STRUCTURES</element>
		<element variable="kTAG_CLASS">COntologyMasterVertex</element>
        <element variable="kTAG_DESCRIPTION"  key="en"><![CDATA[These represent the <i>default structured
        	data types</i>.
        	]]>
        </element>
    </node>
    
    <!-- kTERM_ENUMERATED -->
	<node class="COntologyMasterVertex">
		<element variable="kTAG_TERM">:ENUMERATED</element>
		<element variable="kTAG_CLASS">COntologyMasterVertex</element>
        <element variable="kTAG_DESCRIPTION"  key="en"><![CDATA[These represent the <i>default enumerated
        	data types</i>.
        	]]>
        </element>
    </node>
    
    <!-- kTERM_CARDINALITY -->
	<node class="COntologyMasterVertex">
		<element variable="kTAG_TERM">:CARDINALITY</element>
		<element variable="kTAG_CLASS">COntologyMasterVertex</element>
        <element variable="kTAG_DESCRIPTION"  key="en"><![CDATA[These represent the <i>default cardinality
        	types</i>.
        	]]>
        </element>
    </node>
    
    <!-- kTERM_RELATIONSHIP -->
	<node class="COntologyMasterVertex">
		<element variable="kTAG_TERM">:RELATIONSHIP</element>
		<element variable="kTAG_CLASS">COntologyMasterVertex</element>
        <element variable="kTAG_DESCRIPTION"  key="en"><![CDATA[These represent the <i>default relationship
        	types</i>.
        	]]>
        </element>
    </node>
</nodes>