<?xml version="1.0" encoding="UTF-8"?>
<ONTOLOGY
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="file:/Library/WebServer/Library/PHPWrapper/defines/Ontology.xsd">
    
    <!-- Edge object -->
    
    <UNIT>
        <TERM NS="" LID="_edges">
            <element variable="kTAG_LABEL">
                <item key="en">Edge</item>
            </element>
            <element variable="kTAG_DEFINITION">
                <item key="en"><![CDATA[An ontology edge <i>relates ontology nodes</i> via a predicate. Edges are represented by a triplet: <i>subject</i>, <i>predicate</i> and <i>object</i>, in which the subject and the object are ontology nodes and the predicate is an ontology term.]]></item>
            </element>
        </TERM>
        <NODE class="COntologyMasterVertex">
            <element variable="kTAG_CATEGORY">
                <item>Schema</item>
            </element>
        </NODE>
        <EDGE>
            <element variable="kTAG_PREDICATE">:SUBCLASS-OF</element>
            <element variable="kTAG_OBJECT">:ONTOLOGY:OBJECT</element>
        </EDGE>
    </UNIT>
    
	<!-- kTAG_NID -->
    <UNIT>
        <NODE class="COntologyAliasVertex" term=":NID">
            <element variable="kTAG_PID">:_edges.:NID</element>
            <element variable="kTAG_KIND">
                <item>:KIND-FEATURE</item>
                <item>:KIND-SCALE</item>
            </element>
            <element variable="kTAG_TYPE">
                <item>:INT</item>
                <item>:COMPUTED</item>
                <item>:REQUIRED</item>
            </element>
            <element variable="kTAG_DESCRIPTION">
                <item key="en"><![CDATA[The <i>native identifier</i> of an edge is an <i>integer</i> <i>generated by a sequence</i> or <i>taken from a created graph edge identifier</i>.]]></item>
            </element>
        </NODE>
        <EDGE master="false">
            <element variable="kTAG_SUBJECT" node="pid">:_edges.:NID</element>
            <element variable="kTAG_PREDICATE">:ATTRIBUTE-OF</element>
            <element variable="kTAG_OBJECT">:_edges</element>
        </EDGE>
    </UNIT>
    
	<!-- kTAG_GID -->
    <UNIT>
        <NODE class="COntologyAliasVertex" term=":GID">
            <element variable="kTAG_PID">:_edges.:GID</element>
            <element variable="kTAG_KIND">
                <item>:KIND-FEATURE</item>
                <item>:KIND-SCALE</item>
            </element>
            <element variable="kTAG_TYPE">
                <item>:TEXT</item>
                <item>:LOCKED</item>
                <item>:COMPUTED</item>
                <item>:REQUIRED</item>
            </element>
            <element variable="kTAG_DESCRIPTION">
                <item key="en"><![CDATA[The <i>global identifier</i> of an edge is computed by <i>concatenating</i> the <i>subject vertex native identifier</i>, the <i>predicate global identifier</i> and the <i>object vertex native identifier</i> separated by a slash.]]></item>
            </element>
        </NODE>
        <EDGE master="false">
            <element variable="kTAG_SUBJECT" node="pid">:_edges.:GID</element>
            <element variable="kTAG_PREDICATE">:ATTRIBUTE-OF</element>
            <element variable="kTAG_OBJECT">:_edges</element>
        </EDGE>
    </UNIT>
    
	<!-- kTAG_CLASS -->
    <UNIT>
        <NODE class="COntologyAliasVertex" term=":CLASS">
            <element variable="kTAG_PID">:_edges.:CLASS</element>
            <element variable="kTAG_KIND">
                <item>:KIND-FEATURE</item>
                <item>:KIND-SCALE</item>
            </element>
            <element variable="kTAG_TYPE">
                <item>:TEXT</item>
                <item>:COMPUTED</item>
                <item>:LOCKED</item>
            </element>
        </NODE>
        <EDGE master="false">
            <element variable="kTAG_SUBJECT" node="pid">:_edges.:CLASS</element>
            <element variable="kTAG_PREDICATE">:ATTRIBUTE-OF</element>
            <element variable="kTAG_OBJECT">:_edges</element>
        </EDGE>
    </UNIT>
    
	<!-- kTAG_UID -->
    <UNIT>
        <NODE class="COntologyAliasVertex" term=":UID">
            <element variable="kTAG_PID">:_edges.:UID</element>
            <element variable="kTAG_KIND">
                <item>:KIND-FEATURE</item>
                <item>:KIND-SCALE</item>
            </element>
            <element variable="kTAG_TYPE">
                <item>:BINARY</item>
                <item>:COMPUTED</item>
                <item>:LOCKED</item>
            </element>
        </NODE>
        <EDGE master="false">
            <element variable="kTAG_SUBJECT" node="pid">:_edges.:UID</element>
            <element variable="kTAG_PREDICATE">:ATTRIBUTE-OF</element>
            <element variable="kTAG_OBJECT">:_edges</element>
        </EDGE>
    </UNIT>
    
	<!-- kTAG_SUBJECT -->
    <UNIT>
        <NODE class="COntologyAliasVertex" term=":SUBJECT">
            <element variable="kTAG_PID">:_edges.:SUBJECT</element>
            <element variable="kTAG_KIND">
                <item>:KIND-FEATURE</item>
                <item>:KIND-SCALE</item>
            </element>
            <element variable="kTAG_TYPE">
                <item>:REF-NODE</item>
                <item>:INT</item>
                <item>:LOCKED</item>
                <item>:COMPUTED</item>
                <item>:RESTRICTED</item>
            </element>
            <element variable="kTAG_INPUT">
                <item>:INPUT-TEXT</item>
            </element>
        </NODE>
        <EDGE master="false">
            <element variable="kTAG_SUBJECT" node="pid">:_edges.:SUBJECT</element>
            <element variable="kTAG_PREDICATE">:ATTRIBUTE-OF</element>
            <element variable="kTAG_OBJECT">:_edges</element>
        </EDGE>
    </UNIT>
    
	<!-- kTAG_PREDICATE -->
    <UNIT>
        <NODE class="COntologyAliasVertex" term=":PREDICATE">
            <element variable="kTAG_PID">:_edges.:PREDICATE</element>
            <element variable="kTAG_KIND">
                <item>:KIND-FEATURE</item>
                <item>:KIND-SCALE</item>
            </element>
            <element variable="kTAG_TYPE">
				<item>:REF-TERM</item>
                <item>:BINARY</item>
                <item>:LOCKED</item>
                <item>:REQUIRED</item>
                <item>:RESTRICTED</item>
            </element>
            <element variable="kTAG_INPUT">
                <item>:INPUT-TEXT</item>
            </element>
        </NODE>
        <EDGE master="false">
            <element variable="kTAG_SUBJECT" node="pid">:_edges.:PREDICATE</element>
            <element variable="kTAG_PREDICATE">:ATTRIBUTE-OF</element>
            <element variable="kTAG_OBJECT">:_edges</element>
        </EDGE>
    </UNIT>
    
	<!-- kTAG_OBJECT -->
    <UNIT>
        <NODE class="COntologyAliasVertex" term=":OBJECT">
            <element variable="kTAG_PID">:_edges.:OBJECT</element>
            <element variable="kTAG_KIND">
                <item>:KIND-FEATURE</item>
                <item>:KIND-SCALE</item>
            </element>
            <element variable="kTAG_TYPE">
                <item>:REF-NODE</item>
                <item>:INT</item>
                <item>:LOCKED</item>
                <item>:COMPUTED</item>
                <item>:RESTRICTED</item>
            </element>
            <element variable="kTAG_INPUT">
                <item>:INPUT-TEXT</item>
            </element>
        </NODE>
        <EDGE master="false">
            <element variable="kTAG_SUBJECT" node="pid">:_edges.:OBJECT</element>
            <element variable="kTAG_PREDICATE">:ATTRIBUTE-OF</element>
            <element variable="kTAG_OBJECT">:_edges</element>
        </EDGE>
    </UNIT>
    
	<!-- kTAG_PATH -->
    <UNIT>
        <NODE class="COntologyAliasVertex" term=":PATH">
            <element variable="kTAG_PID">:_edges.:PATH</element>
            <element variable="kTAG_KIND">
                <item>:KIND-FEATURE</item>
                <item>:KIND-SCALE</item>
            </element>
            <element variable="kTAG_TYPE">
                <item>:BINARY</item>
                <item>:COMPUTED</item>
                <item>:LOCKED</item>
                <item>:ARRAY</item>
            </element>
            <element variable="kTAG_DESCRIPTION">
                <item key="en"><![CDATA[The <i>edge path</i> records the <i>subject</i>, <i>predicate</i> and the <i>object</i> of the edge as term references. This attribute represents the <i>term edges</i>]]></item>
            </element>
        </NODE>
        <EDGE master="false">
            <element variable="kTAG_SUBJECT" node="pid">:_edges.:PATH</element>
            <element variable="kTAG_PREDICATE">:ATTRIBUTE-OF</element>
            <element variable="kTAG_OBJECT">:_edges</element>
        </EDGE>
    </UNIT>
    
	<!-- kTAG_PID -->
    <UNIT>
        <NODE class="COntologyAliasVertex" term=":PID">
            <element variable="kTAG_PID">:_edges.:PID</element>
            <element variable="kTAG_KIND">
                <item>:KIND-FEATURE</item>
                <item>:KIND-SCALE</item>
            </element>
            <element variable="kTAG_TYPE">
                <item>:TEXT</item>
                <item>:COMPUTED</item>
                <item>:LOCKED</item>
            </element>
            <element variable="kTAG_INPUT">
                <item>:INPUT-TEXT</item>
            </element>
            <element variable="kTAG_DESCRIPTION">
                <item key="en"><![CDATA[The <i>persistent identifier</i> of an edge is the string representation of the edge's path, it is the <i>path elements global identifiers</i> separated by a slash. This attribute can be useful to debug edges.]]></item>
            </element>
        </NODE>
        <EDGE master="false">
            <element variable="kTAG_SUBJECT" node="pid">:_edges.:PID</element>
            <element variable="kTAG_PREDICATE">:ATTRIBUTE-OF</element>
            <element variable="kTAG_OBJECT">:_edges</element>
        </EDGE>
    </UNIT>
</ONTOLOGY>
