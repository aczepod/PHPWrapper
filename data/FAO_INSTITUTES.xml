<units>
    <!-- FAO-INSTITUTES -->
    <unit>
    	<term lid="FAO-INSTITUTES">
			<element variable="kTAG_KIND">
				<item>:KIND-NAMESPACE</item>
			</element>
			<element variable="kTAG_LABEL" key="en">FAO/WIEWS Institutes</element>
			<element variable="kTAG_DEFINITION" key="en">The WIEWS Institute table contains all the institutions recorded under the System as well as their codes. WIEWS Institution codes are part of the FAO/IPGRI Multicrop descriptor list, which have been internationally agreed to harmonize PGR databases worldwide and simplify data exchange.</element>
    	</term>
    	<node class="COntologyMasterVertex" />
    </unit>
    
    <!-- INSTITUTES PROPERTIES -->
    
    <!-- FAO-INSTITUTES:INSTCODE -->
    <unit>
    	<term lid="INSTCODE" ns="FAO-INSTITUTES">
			<element variable="kTAG_LABEL" key="en">Institute code</element>
			<element variable="kTAG_DEFINITION" key="en">Institute code consisting of the 3-letter ISO 3166 country code ([#ISO:3166:1:alpha-3#]) of the country where the institute is located plus a number. The current set of Institute Codes is available from the FAO website (http://apps3.fao.org/wiews/). Note that although you usually see three digits following the country code, there are actually seven characters in the code, allowing four digits.</element>
    	</term>
    	<node class="COntologyMasterVertex">
			<element variable="kTAG_KIND">
				<item>:KIND-FEATURE</item>
				<item>:KIND-SCALE</item>
			</element>
			<element variable="kTAG_TYPE">
				<item>:TEXT</item>
			</element>
			<element variable="kTAG_EXAMPLES">
				<item>ZWE063</item>
				<item>ITA303</item>
				<item>USA999</item>
			</element>
    	</node>
    	<tag>
			<element variable="kTAG_PATH">
				<item>FAO-INSTITUTES:INSTCODE</item>
			</element>
    	</tag>
        <edges>
			<edge>
				<element variable="kTAG_PREDICATE">:SUBCLASS-OF</element>
				<element variable="kTAG_OBJECT">FAO-INSTITUTES</element>
			</edge>
        </edges>
    </unit>
    
