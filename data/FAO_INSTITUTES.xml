<units>
    <!-- FAO-INSTITUTES -->
    <unit>
    	<term kTAG_LID="FAO-INSTITUTES">
			<kTAG_KIND>
				<item>:KIND-NAMESPACE</item>
			</kTAG_KIND>
			<kTAG_LABEL language="en">FAO/WIEWS Institutes</kTAG_LABEL>
			<kTAG_DEFINITION language="en">The WIEWS Institute table contains all the institutions recorded under the System as well as their codes. WIEWS Institution codes are part of the FAO/IPGRI Multicrop descriptor list, which have been internationally agreed to harmonize PGR databases worldwide and simplify data exchange.</kTAG_DEFINITION>
    	</term>
    	<node>
			<kTAG_CLASS>COntologyMasterVertex</kTAG_CLASS>
 	  	</node>
    </unit>
    
    <!-- INSTITUTES PROPERTIES -->
    
    <!-- FAO-INSTITUTES:INSTCODE -->
    <unit>
    	<term kTAG_LID="INSTCODE" kTAG_NAMESPACE="FAO-INSTITUTES">
			<kTAG_LABEL language="en">Institute code</kTAG_LABEL>
			<kTAG_DEFINITION language="en">Institute code consisting of the 3-letter ISO 3166 country code ([#ISO:3166:1:alpha-3#]) of the country where the institute is located plus a number. The current set of Institute Codes is available from the FAO website (http://apps3.fao.org/wiews/). Note that although you usually see three digits following the country code, there are actually seven characters in the code, allowing four digits.</kTAG_DEFINITION>
    	</term>
    	<node>
			<kTAG_CLASS>COntologyMasterVertex</kTAG_CLASS>
			<kTAG_KIND>
				<item>:KIND-FEATURE</item>
				<item>:KIND-SCALE</item>
			</kTAG_KIND>
			<kTAG_TYPE>
				<item>:TEXT</item>
			</kTAG_TYPE>
			<kTAG_EXAMPLES>
				<item>ZWE063</item>
				<item>ITA303</item>
				<item>USA999</item>
			</kTAG_EXAMPLES>
    	</node>
    	<tag>
    		<item>FAO-INSTITUTES:INSTCODE</item>
    	</tag>
        <edges>
			<edge>
				<kTAG_PREDICATE>:SUBCLASS-OF</kTAG_PREDICATE>
				<kTAG_OBJECT>FAO-INSTITUTES</kTAG_OBJECT>
			</edge>
        </edges>
    </unit>
    
