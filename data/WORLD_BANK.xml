	<!-- ATTRIBUTES -->

	<UNIT>
		<TERM LID="GROUP" NS="WBI">
			<element variable="kTAG_LABEL">
				<item key="en">Classification groups</item>
			</element>
			<element variable="kTAG_DEFINITION">
				<item key="en">For operational and analytical purposes, the World Bankâ€™s main criterion for classifying economies is gross national income (GNI) per capita. In previous editions of our publications, this term was referred to as gross national product, or GNP. Based on its GNI per capita, every economy is classified as low income, middle income (subdivided into lower middle and upper middle), or high income. Other analytical groups based on geographic regions are also used.</item>
			</element>
		</TERM>
		<NODE class="COntologyMasterVertex">
			<element variable="kTAG_KIND">
			    <item>:KIND-FEATURE</item>
			    <item>:KIND-SCALE</item>
			</element>
			<element variable="kTAG_TYPE">
				<item>:SET</item>
			</element>
		</NODE>
		<EDGE>
			<element variable="kTAG_PREDICATE">:SUBCLASS-OF</element>
			<element variable="kTAG_OBJECT">WBI</element>
		</EDGE>
    	<TAG>
			<element variable="kTAG_PATH">
    			<item node="term">WBI:GROUP</item>
			</element>
    	</TAG>
	</UNIT>

	<UNIT>
		<TERM LID="VERSION" NS="WBI">
			<element variable="kTAG_LABEL">
				<item key="en">Version</item>
			</element>
			<element variable="kTAG_DEFINITION">
				<item key="en">Classification version.</item>
			</element>
		</TERM>
		<NODE class="COntologyMasterVertex">
			<element variable="kTAG_KIND">
			    <item>:KIND-FEATURE</item>
			    <item>:KIND-SCALE</item>
			</element>
