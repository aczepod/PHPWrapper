			</element>
		</NODE>
		<EDGE>
			<element variable="kTAG_PREDICATE">:SUBCLASS-OF</element>
			<element variable="kTAG_OBJECT">WBI</element>
		</EDGE>
    	<TAG>
			<element variable="kTAG_PATH">
    			<item node="term">WBI:VERSION</item>
			</element>
    	</TAG>
	</UNIT>

