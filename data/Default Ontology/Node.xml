                <item>:KIND-SCALE</item>
            </element>
            <element variable="kTAG_TYPE">
                <item>:TEXT</item>
                <item>:COMPUTED</item>
                <item>:LOCKED</item>
            </element>
        </NODE>
        <EDGE master="false">
            <element variable="kTAG_SUBJECT" node="pid">:_nodes.:CLASS</element>
            <element variable="kTAG_PREDICATE">:ATTRIBUTE-OF</element>
            <element variable="kTAG_OBJECT">:_nodes</element>
        </EDGE>
    </UNIT>
    
	<!-- kTAG_PID -->
    <UNIT>
        <NODE class="COntologyAliasVertex" term=":PID">
            <element variable="kTAG_PID">:_nodes.:PID</element>
            <element variable="kTAG_KIND">
                <item>:KIND-FEATURE</item>
                <item>:KIND-SCALE</item>
            </element>
            <element variable="kTAG_TYPE">
                <item>:TEXT</item>
                <item>:LOCKED</item>
            </element>
            <element variable="kTAG_INPUT">
                <item>:INPUT-TEXT</item>
            </element>
            <element variable="kTAG_DESCRIPTION">
                <item key="en"><![CDATA[The <i>persistent identifier</i> of a node is a user provided string which must be unique among all nodes. This string is persistent, while the node's native identifier is not: using this attribute it is possible to reference alias nodes without worrying that the native identifier changes across implementations.]]></item>
            </element>
        </NODE>
        <EDGE master="false">
            <element variable="kTAG_SUBJECT" node="pid">:_nodes.:PID</element>
            <element variable="kTAG_PREDICATE">:ATTRIBUTE-OF</element>
            <element variable="kTAG_OBJECT">:_nodes</element>
        </EDGE>
    </UNIT>
    
