<?xml version="1.0" encoding="UTF-8"?>
<ONTOLOGY
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="file:/Library/WebServer/Library/PHPWrapper/defines/Ontology.xsd">
    
    <!-- Namespaces -->
    
    <UNIT>
		<!-- Default namespace -->
        <TERM LID="">
            <element variable="kTAG_KIND">
                <item>:KIND-NAMESPACE</item>
            </element>
            <element variable="kTAG_LABEL">
                <item key="en">Default namespace</item>
            </element>
            <element variable="kTAG_DEFINITION">
                <item key="en"><![CDATA[This <i>namespace</i> groups all default terms of the ontology, these are the elements that will be used to build the ontology itself.]]></item>
            </element>
        </TERM>
    
		<!-- Status namespace -->
        <TERM NS="" LID="STATUS">
            <element variable="kTAG_KIND">
                <item>:KIND-NAMESPACE</item>
            </element>
            <element variable="kTAG_LABEL">
                <item key="en">Status</item>
            </element>
            <element variable="kTAG_DEFINITION">
                <item key="en"><![CDATA[This <i>namespace</i> groups all terms which define a <i>status level</i> or <i>type</i>.]]></item>
            </element>
        </TERM>
    </UNIT>
 </ONTOLOGY>
