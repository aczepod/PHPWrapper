<?xml version="1.0" encoding="utf-8"?>
<ONTOLOGY
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="file:/Library/WebServer/Library/PHPWrapper/defines/Ontology.xsd">
    
    <!-- GR -->
    <UNIT>
    	<TERM LID="GR">
			<element variable="kTAG_KIND">
				<item>:KIND-NAMESPACE</item>
			</element>
			<element variable="kTAG_LABEL">
				<item key="en">Genetic Resources INFOrmation</item>
			</element>
			<element variable="kTAG_DEFINITION">
				<item key="en">Features and scales generic to various genetic resource descriptors.</item>
			</element>
    	</TERM>
    </UNIT>
    
    <!-- GR:NICODE -->
    <UNIT>
    	<TERM LID="NICODE" NS="GR">
			<element variable="kTAG_LABEL">
				<item key="en">National Inventory Code</item>
			</element>
			<element variable="kTAG_DEFINITION">
				<item key="en">Country code identifying the National Inventory, it is represented by the 3-letter ISO 3166-1 country code ([#ISO:3166:1:alpha-3#]) of the inventory country.</item>
			</element>
    	</TERM>
    	<NODE class="COntologyMasterVertex">
            <element variable="kTAG_PID">GR:NICODE</element>
			<element variable="kTAG_KIND">
				<item>:KIND-FEATURE</item>
				<item>:KIND-SCALE</item>
			</element>
			<element variable="kTAG_TYPE">
				<item>:ENUM</item>
			</element>
    	</NODE>
    	<TAG>
    		<element variable="kTAG_PATH">
    			<item node="pid">GR:NICODE</item>
    		</element>
    	</TAG>
    </UNIT>
    
    <!-- GR:GENUS -->
    <UNIT>
    	<TERM LID="GENUS" NS="GR">
			<element variable="kTAG_LABEL">
				<item key="en">Genus</item>
			</element>
			<element variable="kTAG_DEFINITION">
				<item key="en">Generic epithet biological classification.</item>
			</element>
			<element variable="kTAG_SYNONYMS">
				<item>GENUS</item>
			</element>
    	</TERM>
    	<NODE class="COntologyMasterVertex">
            <element variable="kTAG_PID">GR:GENUS</element>
			<element variable="kTAG_KIND">
				<item>:KIND-FEATURE</item>
				<item>:KIND-SCALE</item>
			</element>
			<element variable="kTAG_TYPE">
				<item>:TEXT</item>
			</element>
    	</NODE>
    	<TAG>
    		<element variable="kTAG_PATH">
    			<item node="pid">GR:GENUS</item>
    		</element>
    	</TAG>
    </UNIT>
    
    <!-- GR:SPECIES -->
    <UNIT>
    	<TERM LID="SPECIES" NS="GR">
			<element variable="kTAG_LABEL">
				<item key="en">Species</item>
			</element>
			<element variable="kTAG_DEFINITION">
				<item key="en">Specific epithet biological classification.</item>
			</element>
			<element variable="kTAG_SYNONYMS">
				<item>SPECIES</item>
			</element>
    	</TERM>
    	<NODE class="COntologyMasterVertex">
            <element variable="kTAG_PID">GR:SPECIES</element>
			<element variable="kTAG_KIND">
				<item>:KIND-FEATURE</item>
				<item>:KIND-SCALE</item>
			</element>
			<element variable="kTAG_TYPE">
				<item>:TEXT</item>
			</element>
    	</NODE>
    	<TAG>
    		<element variable="kTAG_PATH">
    			<item node="pid">GR:SPECIES</item>
    		</element>
    	</TAG>
    </UNIT>
    
    <!-- GR:SPAUTHOR -->
    <UNIT>
    	<TERM LID="SPAUTHOR" NS="GR">
			<element variable="kTAG_LABEL">
				<item key="en">Species authority</item>
			</element>
			<element variable="kTAG_DEFINITION">
				<item key="en">Name or abbreviation of the author of the specific epithet biological classification.</item>
			</element>
			<element variable="kTAG_SYNONYMS">
				<item>SPAUTHOR</item>
			</element>
    	</TERM>
    	<NODE class="COntologyMasterVertex">
            <element variable="kTAG_PID">GR:SPECIES</element>
			<element variable="kTAG_KIND">
				<item>:KIND-FEATURE</item>
				<item>:KIND-SCALE</item>
			</element>
			<element variable="kTAG_TYPE">
				<item>:TEXT</item>
			</element>
    	</NODE>
    	<TAG>
    		<element variable="kTAG_PATH">
    			<item node="pid">GR:SPECIES</item>
    		</element>
    	</TAG>
    </UNIT>
    
    <!-- GR:SUBTAXA -->
    <UNIT>
    	<TERM LID="SUBTAXA" NS="GR">
			<element variable="kTAG_LABEL">
				<item key="en">Infraspecific epithet</item>
			</element>
			<element variable="kTAG_DEFINITION">
				<item key="en">The third word in the scientific name of an infraspecific taxon, following the name of the species.</item>
			</element>
			<element variable="kTAG_SYNONYMS">
				<item>SUBTAXA</item>
			</element>
    	</TERM>
    	<NODE class="COntologyMasterVertex">
            <element variable="kTAG_PID">GR:SUBTAXA</element>
			<element variable="kTAG_KIND">
				<item>:KIND-FEATURE</item>
				<item>:KIND-SCALE</item>
			</element>
			<element variable="kTAG_TYPE">
				<item>:TEXT</item>
			</element>
    	</NODE>
    	<TAG>
    		<element variable="kTAG_PATH">
    			<item node="pid">GR:SUBTAXA</item>
    		</element>
    	</TAG>
    </UNIT>
    
    <!-- GR:SUBTAUTHOR -->
    <UNIT>
    	<TERM LID="SUBTAUTHOR" NS="GR">
			<element variable="kTAG_LABEL">
				<item key="en">Infraspecific epithet authority</item>
			</element>
			<element variable="kTAG_DEFINITION">
				<item key="en">Name or abbreviation of the author of the third word in the scientific name of an infraspecific taxon which follows the name of the species.</item>
			</element>
			<element variable="kTAG_SYNONYMS">
				<item>SUBTAUTHOR</item>
			</element>
    	</TERM>
    	<NODE class="COntologyMasterVertex">
            <element variable="kTAG_PID">GR:SUBTAUTHOR</element>
			<element variable="kTAG_KIND">
				<item>:KIND-FEATURE</item>
				<item>:KIND-SCALE</item>
			</element>
			<element variable="kTAG_TYPE">
				<item>:TEXT</item>
			</element>
    	</NODE>
    	<TAG>
    		<element variable="kTAG_PATH">
    			<item node="pid">GR:SUBTAUTHOR</item>
    		</element>
    	</TAG>
    </UNIT>
    
    <!-- GR:DATE -->
    <UNIT>
    	<TERM LID="DATE" NS="GR">
			<element variable="kTAG_LABEL">
				<item key="en"><![CDATA[Date string]]></item>
			</element>
			<element variable="kTAG_DEFINITION">
				<item key="en"><![CDATA[A date expressed as a YYYYMMDD string in which the days and months may be omitted. Missing elements are omitted from the storewd value, while missing elements may be provided by replacing them with hyphens, <tt>--</tt>, or zeros, <tt>00</tt>.]]></item>
			</element>
    	</TERM>
    	<NODE class="COntologyMasterVertex">
            <element variable="kTAG_PID">GR:DATE</element>
			<element variable="kTAG_KIND">
				<item>:KIND-FEATURE</item>
				<item>:KIND-SCALE</item>
			</element>
			<element variable="kTAG_TYPE">
				<item>:TEXT</item>
			</element>
			<element variable="kTAG_EXAMPLES">
				<item><![CDATA[Full date: <tt>19781207</tt>]]></item>
				<item><![CDATA[Missing days: <tt>197812</tt> or <tt>197812--</tt> or <tt>19781200</tt>]]></item>
				<item><![CDATA[Missing months: <tt>1978</tt> or <tt>1978----</tt> or <tt>19780000</tt>]]></item>
			</element>
    	</NODE>
    	<TAG>
    		<element variable="kTAG_PATH">
    			<item node="pid">GR:DATE</item>
    		</element>
    	</TAG>
    </UNIT>
    
    <!-- GR:LATITUDE -->
    <UNIT>
    	<TERM LID="LATITUDE" NS="GR">
			<element variable="kTAG_LABEL">
				<item key="en"><![CDATA[Latitude]]></item>
			</element>
			<element variable="kTAG_DEFINITION">
				<item key="en"><![CDATA[Latitude expressed in degrees, minutes and seconds in a <tt>DDMMSSH</tt> string, in which <tt>DD</tt> expresses the degrees ±90, <tt>MM</tt> expresses the minutes ± 60, <tt>SS</tt> expresses the seconds ± 60 and <tt>HH</tt> expresses the hemisphere <tt>N</TT> or <tt>S</tt>. The coordinate may lack the seconds and the minutes, in that case the missing elements are not recorded, while they can be provided as hyphens, <tt>--</tt>.]]></item>
			</element>
    	</TERM>
    	<NODE class="COntologyMasterVertex">
            <element variable="kTAG_PID">GR:LATITUDE</element>
			<element variable="kTAG_KIND">
				<item>:KIND-FEATURE</item>
				<item>:KIND-SCALE</item>
			</element>
			<element variable="kTAG_TYPE">
				<item>:TEXT</item>
			</element>
			<element variable="kTAG_EXAMPLES">
				<item><![CDATA[Full latitude: <tt>011539N</tt>]]></item>
				<item><![CDATA[Missing seconds: <tt>0115N</tt> or <tt>0115--N</tt>]]></item>
				<item><![CDATA[Missing minutes: <tt>01N</tt> or <tt>01----N</tt>]]></item>
			</element>
			<element variable="kTAG_DESCRIPTION">
				<item key="en"><![CDATA[To convert from longitude and latitude in degrees (o), minutes ('), seconds (''), and a hemisphere (North or South and East or West) to decimal degrees, the following formula should be used: do m' s''=h *(d+m/ 60+s/3600) where h=1 for the Northern and Eastern hemispheres and –1 for the Southern and Western hemispheres i.e. 30o30'0'' S= –30.5 and 30o15'55'' N=30.265.]]></item>
			</element>
    	</NODE>
    	<TAG>
    		<element variable="kTAG_PATH">
    			<item node="pid">GR:LATITUDE</item>
    		</element>
    	</TAG>
    </UNIT>
    
    <!-- GR:LONGITUDE -->
    <UNIT>
    	<TERM LID="LONGITUDE" NS="GR">
			<element variable="kTAG_LABEL">
				<item key="en"><![CDATA[Longitude]]></item>
			</element>
			<element variable="kTAG_DEFINITION">
				<item key="en"><![CDATA[Longitude expressed in degrees, minutes and seconds in a <tt>DDDMMSSH</tt> string, in which <tt>DD</tt> expresses the degrees ±180, <tt>MM</tt> expresses the minutes ± 60, <tt>SS</tt> expresses the seconds ± 60 and <tt>HH</tt> expresses the hemisphere <tt>E</TT> or <tt>W</tt>. The coordinate may lack the seconds and the minutes, in that case the missing elements are not recorded, while they can be provided as hyphens, <tt>--</tt>.]]></item>
			</element>
    	</TERM>
    	<NODE class="COntologyMasterVertex">
            <element variable="kTAG_PID">GR:LONGITUDE</element>
			<element variable="kTAG_KIND">
				<item>:KIND-FEATURE</item>
				<item>:KIND-SCALE</item>
			</element>
			<element variable="kTAG_TYPE">
				<item>:TEXT</item>
			</element>
			<element variable="kTAG_EXAMPLES">
				<item><![CDATA[Full longitude: <tt>0762510W</tt>]]></item>
				<item><![CDATA[Missing seconds: <tt>07625W</tt> or <tt>07625--W</tt>]]></item>
				<item><![CDATA[Missing minutes: <tt>076W</tt> or <tt>076----W</tt>]]></item>
			</element>
			<element variable="kTAG_DESCRIPTION">
				<item key="en"><![CDATA[To convert from longitude and latitude in degrees (o), minutes ('), seconds (''), and a hemisphere (North or South and East or West) to decimal degrees, the following formula should be used: do m' s''=h *(d+m/ 60+s/3600) where h=1 for the Northern and Eastern hemispheres and –1 for the Southern and Western hemispheres i.e. 30o30'0'' S= –30.5 and 30o15'55'' N=30.265.]]></item>
			</element>
    	</NODE>
    	<TAG>
    		<element variable="kTAG_PATH">
    			<item node="pid">GR:LONGITUDE</item>
    		</element>
    	</TAG>
    </UNIT>
    
    <!-- GR:COUNTRIES -->
    <UNIT>
    	<TERM LID="COUNTRIES" NS="GR">
			<element variable="kTAG_LABEL">
				<item key="en"><![CDATA[Countries]]></item>
			</element>
			<element variable="kTAG_DEFINITION">
				<item key="en"><![CDATA[Full country list. This list includes both the current and obsolete countries, this list should be used whenever historic information is important, in other words, whenevee the originally recorded country must be conserved>.]]></item>
			</element>
    	</TERM>
    	<NODE class="COntologyMasterVertex">
            <element variable="kTAG_PID">GR:COUNTRIES</element>
			<element variable="kTAG_KIND">
				<item>:KIND-FEATURE</item>
				<item>:KIND-SCALE</item>
			</element>
			<element variable="kTAG_TYPE">
				<item>:ENUM</item>
			</element>
    	</NODE>
    	<TAG>
    		<element variable="kTAG_PATH">
    			<item node="pid">GR:COUNTRIES</item>
    		</element>
    	</TAG>
    </UNIT>
</ONTOLOGY>