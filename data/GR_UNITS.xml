    <!-- GR:DATE -->
    <unit>
    	<term kTAG_LID="DATE" kTAG_NAMESPACE="GR">
			<kTAG_LABEL language="en"><![CDATA[Date string]]></kTAG_LABEL>
			<kTAG_DEFINITION language="en"><![CDATA[A date expressed as a YYYYMMDD string in which the days and months may be omitted. Missing elements are omitted from the storewd value, while missing elements may be provided by replacing them with hyphens, <tt>--</tt>, or zeros, <tt>00</tt>.]]></kTAG_DEFINITION>
    	</term>
    	<node>
			<kTAG_CLASS>COntologyMasterVertex</kTAG_CLASS>
			<kTAG_KIND>
				<item>:KIND-SCALE</item>
			</kTAG_KIND>
			<kTAG_TYPE>
				<item>:TEXT</item>
			</kTAG_TYPE>
			<kTAG_EXAMPLES>
				<item><![CDATA[Full date: <tt>19781207</tt>]]></item>
				<item><![CDATA[Missing days: <tt>197812</tt> or <tt>197812--</tt> or <tt>19781200</tt>]]></item>
				<item><![CDATA[Missing months: <tt>1978</tt> or <tt>1978----</tt> or <tt>19780000</tt>]]></item>
			</kTAG_EXAMPLES>
    	</node>
        <edges>
			<edge>
				<kTAG_PREDICATE>:SUBCLASS-OF</kTAG_PREDICATE>
				<kTAG_OBJECT>GR</kTAG_OBJECT>
			</edge>
        </edges>
    </unit>
    
    <!-- GR:LATITUDE -->
    <unit>
    	<term kTAG_LID="LATITUDE" kTAG_NAMESPACE="GR">
			<kTAG_LABEL language="en"><![CDATA[Latitude]]></kTAG_LABEL>
			<kTAG_DEFINITION language="en"><![CDATA[Latitude expressed in degrees, minutes and seconds in a <tt>DDMMSSH</tt> string, in which <tt>DD</tt> expresses the degrees ±90, <tt>MM</tt> expresses the minutes ± 60, <tt>SS</tt> expresses the seconds ± 60 and <tt>HH</tt> expresses the hemisphere <tt>N</TT> or <tt>S</tt>. The coordinate may lack the seconds and the minutes, in that case the missing elements are not recorded, while they can be provided as hyphens, <tt>--</tt>.]]></kTAG_DEFINITION>
    	</term>
    	<node>
			<kTAG_CLASS>COntologyMasterVertex</kTAG_CLASS>
			<kTAG_KIND>
				<item>:KIND-SCALE</item>
			</kTAG_KIND>
			<kTAG_TYPE>
				<item>:TEXT</item>
			</kTAG_TYPE>
			<kTAG_EXAMPLES>
				<item><![CDATA[Full latitude: <tt>011539N</tt>]]></item>
				<item><![CDATA[Missing seconds: <tt>0115N</tt> or <tt>0115--N</tt>]]></item>
				<item><![CDATA[Missing minutes: <tt>01N</tt> or <tt>01----N</tt>]]></item>
			</kTAG_EXAMPLES>
			<kTAG_DESCRIPTION language="en"><![CDATA[To convert from longitude and latitude in degrees (o), minutes ('), seconds (''), and a hemisphere (North or South and East or West) to decimal degrees, the following formula should be used: do m' s''=h *(d+m/ 60+s/3600) where h=1 for the Northern and Eastern hemispheres and –1 for the Southern and Western hemispheres i.e. 30o30'0'' S= –30.5 and 30o15'55'' N=30.265.]]></kTAG_DESCRIPTION>
    	</node>
        <edges>
			<edge>
				<kTAG_PREDICATE>:SUBCLASS-OF</kTAG_PREDICATE>
				<kTAG_OBJECT>GR</kTAG_OBJECT>
			</edge>
        </edges>
    </unit>
    
    <!-- GR:LONGITUDE -->
    <unit>
    	<term kTAG_LID="LONGITUDE" kTAG_NAMESPACE="GR">
			<kTAG_LABEL language="en"><![CDATA[Longitude]]></kTAG_LABEL>
			<kTAG_DEFINITION language="en"><![CDATA[Longitude expressed in degrees, minutes and seconds in a <tt>DDDMMSSH</tt> string, in which <tt>DD</tt> expresses the degrees ±180, <tt>MM</tt> expresses the minutes ± 60, <tt>SS</tt> expresses the seconds ± 60 and <tt>HH</tt> expresses the hemisphere <tt>E</TT> or <tt>W</tt>. The coordinate may lack the seconds and the minutes, in that case the missing elements are not recorded, while they can be provided as hyphens, <tt>--</tt>.]]></kTAG_DEFINITION>
    	</term>
    	<node>
			<kTAG_CLASS>COntologyMasterVertex</kTAG_CLASS>
			<kTAG_KIND>
				<item>:KIND-SCALE</item>
			</kTAG_KIND>
			<kTAG_TYPE>
				<item>:TEXT</item>
			</kTAG_TYPE>
			<kTAG_EXAMPLES>
				<item><![CDATA[Full longitude: <tt>0762510W</tt>]]></item>
				<item><![CDATA[Missing seconds: <tt>07625W</tt> or <tt>07625--W</tt>]]></item>
				<item><![CDATA[Missing minutes: <tt>076W</tt> or <tt>076----W</tt>]]></item>
			</kTAG_EXAMPLES>
			<kTAG_DESCRIPTION language="en"><![CDATA[To convert from longitude and latitude in degrees (o), minutes ('), seconds (''), and a hemisphere (North or South and East or West) to decimal degrees, the following formula should be used: do m' s''=h *(d+m/ 60+s/3600) where h=1 for the Northern and Eastern hemispheres and –1 for the Southern and Western hemispheres i.e. 30o30'0'' S= –30.5 and 30o15'55'' N=30.265.]]></kTAG_DESCRIPTION>
    	</node>
        <edges>
			<edge>
				<kTAG_PREDICATE>:SUBCLASS-OF</kTAG_PREDICATE>
				<kTAG_OBJECT>GR</kTAG_OBJECT>
			</edge>
        </edges>
    </unit>
